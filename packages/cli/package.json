{
  "private": true,
  "name": "@straw-hat/cli",
  "version": "0.0.0",
  "description": "Straw Hat Team Platform CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/javascript.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "oclif",
    "cli",
    "straw-hat-team"
  ],
  "oclif": {
    "commands": "./dist/commands",
    "bin": "shc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "prepack": "yarn build && yarn oclif:manifest && yarn oclif:readme",
    "postpack": "rm -f oclif.manifest.json",
    "prebuild": "rimraf dist",
    "build": "tsc -b",
    "start": "tsc -w",
    "oclif:manifest": "oclif-dev manifest dist",
    "oclif:readme": "oclif-dev readme --multi  --dir=docs/commands"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "bin": {
    "shc": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.13.3",
    "@oclif/plugin-help": "2.2.1",
    "envinfo": "7.4.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@oclif/test": "1.2.5"
  }
}
