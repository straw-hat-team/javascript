{
  "private": true,
  "name": "@straw-hat/fetcher",
  "version": "2.5.0",
  "description": "Simple HTTP Client",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/javascript.git",
    "directory": "packages/fetcher"
  },
  "keywords": [
    "http",
    "http-client",
    "fetch",
    "redux-middleware"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn build:esm && yarn build:cjs && yarn build:copy-files",
    "build:esm": "tsc -p tsconfig.prod.json",
    "build:cjs": "tsc -p tsconfig.prod.cjs.json",
    "build:copy-files": "node ../../scripts/copyFiles.js",
    "release": "yarn build && yarn publish dist"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "24.0.21",
    "jest-fetch-mock": "2.1.2"
  }
}
