{
  "name": "@straw-hat/chainable-config",
  "version": "1.0.0",
  "description": "Chainable config like webpack-chain",
  "author": "Straw Hat Team <opensource@straw-hat-llc.com>",
  "repository": "https://github.com/straw-hat-team/chainable-config",
  "license": "MIT",
  "keywords": [
    "webpack-chain",
    "config",
    "chainable",
    "straw-hat-team"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "yarn build:clean",
    "prepare": "yarn build",
    "build:clean": "shc lib:clean",
    "build:types": "tsc -p tsconfig.prod.json",
    "build:compile": "babel src --out-dir dist --extensions '.ts,.tsx,.js,.jsx' --source-maps",
    "build": "yarn build:compile",
    "postbuild": "yarn build:types",
    "start:types": "yarn build:types --watch",
    "start:compile": "yarn build:compile --watch",
    "start": "run-s start:*",
    "test": "shc test"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "deepmerge": "^4.2.2",
    "is-mergeable-object": "^1.1.1"
  }
}
