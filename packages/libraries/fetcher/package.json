{
  "private": true,
  "name": "@straw-hat/fetcher",
  "version": "2.5.1-alpha.2",
  "description": "Simple HTTP Client",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/javascript.git",
    "directory": "packages/libraries/fetcher"
  },
  "license": "MIT",
  "keywords": [
    "http",
    "http-client",
    "fetch",
    "middleware",
    "straw-hat-team"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "yarn build:clean",
    "prepare": "yarn build",
    "build:clean": "rimraf dist",
    "build:types": "tsc -p tsconfig.prod.json --emitDeclarationOnly",
    "build:compile": "babel src --out-dir dist --extensions '.ts,.tsx,.js,.jsx'",
    "build": "yarn build:compile && yarn build:types",
    "test": "shc test"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "devDependencies": {
    "jest-fetch-mock": "^2.1.2"
  }
}
