{
  "name": "@straw-hat/fetcher",
  "version": "3.0.0",
  "description": "Simple HTTP Client",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/javascript.git",
    "directory": "packages/libraries/fetcher"
  },
  "license": "MIT",
  "keywords": [
    "http",
    "http-client",
    "fetch",
    "middleware",
    "straw-hat-team"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "yarn build:clean",
    "prepare": "yarn build",
    "build:clean": "shc lib:clean",
    "build:types": "tsc -p tsconfig.prod.json",
    "build:compile": "babel src --out-dir dist --extensions '.ts,.tsx,.js,.jsx' --source-maps",
    "build": "yarn build:compile",
    "postbuild": "yarn build:types",
    "start:types": "yarn build:types --watch",
    "start:compile": "yarn build:compile --watch",
    "start": "run-s start:*",
    "test": "shc test"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "jest-fetch-mock": "^2.1.2"
  }
}
