{
  "private": true,
  "name": "@straw-hat/jest-config-chain",
  "version": "1.0.0",
  "description": "Config Chain for Jest",
  "author": "Straw Hat Team <opensource@straw-hat-llc.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/javascript.git",
    "directory": "packages/libraries/jest-config-chain"
  },
  "license": "MIT",
  "keywords": [
    "config",
    "chainable",
    "config-chain",
    "jest",
    "straw-hat-team"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "yarn build:clean",
    "prepare": "yarn build",
    "build:clean": "rimraf dist",
    "build:types": "tsc -p tsconfig.prod.json",
    "build:compile": "babel src --out-dir dist --extensions '.ts,.tsx,.js,.jsx' --source-maps",
    "build": "yarn build:compile && yarn build:types",
    "start:types": "yarn build:types --watch",
    "start:compile": "yarn build:compile --watch",
    "start": "run-s start:*"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@straw-hat/chainable-config": "^1.0.0"
  }
}
