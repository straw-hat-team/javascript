{
  "private": true,
  "name": "@straw-hat/cli",
  "version": "0.0.1",
  "description": "Straw Hat Team Platform CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/javascript.git",
    "directory": "packages/tooling/cli"
  },
  "keywords": [
    "oclif",
    "cli",
    "straw-hat-team"
  ],
  "oclif": {
    "commands": "./dist/commands",
    "bin": "shc",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "prepack": "yarn build && yarn oclif:manifest && yarn oclif:readme",
    "postpack": "rm -f oclif.manifest.json",
    "prebuild": "rimraf dist",
    "start": "tsc -w",
    "build": "tsc -b",
    "oclif:manifest": "oclif-dev manifest dist",
    "oclif:readme": "oclif-dev readme --multi  --dir=docs/commands"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "bin": {
    "shc": "bin/run"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "docs",
    "oclif.manifest.json",
    "README.md"
  ],
  "dependencies": {
    "@jest/types": "^24.9.0",
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.13.3",
    "@oclif/plugin-autocomplete": "0.1.4",
    "@oclif/plugin-help": "2.2.1",
    "@oclif/plugin-plugins": "1.7.8",
    "chalk": "^3.0.0",
    "ci-info": "^2.0.0",
    "debug": "4.1.1",
    "del": "5.1.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "enquirer": "2.3.2",
    "envinfo": "7.5.0",
    "jest-cli": "^24.9.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@oclif/test": "1.2.5"
  }
}
